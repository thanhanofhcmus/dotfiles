font_family      FiraCode Nerd Font
bold_font        auto
italic_font      auto
bold_italic_font auto

font_size 14.0

disable_ligatures never

mouse_hide_wait 0

copy_on_select no

strip_trailing_spaces never

select_by_word_characters @-./_~?&=%+#

repaint_delay 10
input_delay 3
sync_to_monitor yes

cursor_trail 0

# BEGIN_KITTY_THEME
# Darkside
include current-theme.conf
# END_KITTY_THEME
